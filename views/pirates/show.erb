<h1><%= @pirate.name %></h1>
<h3>Weight: <%= @pirate.weight %></h3>
<h3>Height: <%= @pirate.height %></h3>
<h3>Ships:</h3>
<% @ships.each do |ship| %>
  <div class="container">
      <h4><%= ship.name %></h4>
      <h4><%= ship.type %></h4>
      <h4><%= ship.booty %></h4>
  </div>
<% end %>
<h2>Display your second ship here</h2>
