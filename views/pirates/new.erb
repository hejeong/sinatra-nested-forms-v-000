<form action="/pirates" method="post">
    <label for="pirate_name">Pirate's Name: </label>
    <input id="pirate_name" type="text" name="pirate[name]">
    <label for="pirate_weight">Pirate's Weight: </label>
    <input id="pirate_weight" type="text" name="pirate[weight]">
    <label for="pirate_height">Pirate's Height: </label>
    <input id="pirate_height" type="text" name="pirate[height]">
    <label for="pirate[ships][][name]">Pirate's 1st Ship Name: </label>
    <input type="text" name="pirate[ships][][name]">
    <label for="pirate[ships][][type]">Pirate's 1st Ship Type: </label>
    <input type="text" name="pirate[ships][][type]">
    <label for="pirate[ships][][booty]">Pirate's 1st Ship Booty: </label>
    <input type="text" name="pirate[ships][][booty]">
    <label for="pirate[ships][][name]">Pirate's 2nd Ship Name: </label>
    <input type="text" name="pirate[ships][][name]">
    <label for="pirate[ships][][type]">Pirate's 2nd Ship Type: </label>
    <input type="text" name="pirate[ships][][type]">
    <label for="pirate[ships][][booty]">Pirate's 2nd Ship Booty: </label>
    <input type="text" name="pirate[ships][][booty]">
    <input type="submit" value="submit">
</form>
